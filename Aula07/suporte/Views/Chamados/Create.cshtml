@model chamados.Models.Chamado

<div class="form-horizontal">
    <form action="Create" method="Post">

        <br>

        <div class="form-group">
            <label>Id</label>
            <input  class="form-control"
                    type="text" name="id"/>
        </div>

        <div class="form-group">
            <label>NÃºmero Chamado</label>
            <input  class="form-control"
                    type="text" name="numChamado"/>
        </div>

        <div class="form-group">
            <label>Numero Solicitante</label>
            <input  class="form-control"
                    type="text" name="numSolicitante"/>
        </div>

        <div class="form-group">
            <label>Descricao do Problema</label>
            <input  class="form-control"
                    type="text" name="descricaoProblema"/>
        </div>

        <div class="form-group">
            <label>Email</label>
            <input  class="form-control"
                    type="text" name="email"/>
        </div>

